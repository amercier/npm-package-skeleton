{
  "name": "package-skeleton",
  "version": "1.0.4",
  "description": "Skeleton of a simple NPM Package with Babel and CI already configured.",
  "license": "ISC",
  "repository": "amercier/npm-package-skeleton",
  "author": "Alex Mercier <pro.alexandre.mercier@gmail.com> (http://amercier.com)",
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "transpile": "rimraf dist && babel src -d dist",
    "prepublish": "npm run transpile",
    "doc": "rimraf esdoc && esdoc -c esdoc.json",
    "lint": "eslint .",
    "karma": "karma start",
    "istanbul": "istanbul report --root coverage html lcovonly text",
    "start": "export NODE_ENV=development; npm run karma",
    "test": "export NODE_ENV=test; npm run lint && rimraf coverage && npm run karma -- --single-run --no-auto-watch && npm run istanbul"
  },
  "files": [
    "dist"
  ],
  "main": "dist/lib/index.js",
  "keywords": [
    "package",
    "skeleton",
    "example",
    "node",
    "npm",
    "babel",
    "ci",
    "code",
    "codeclimate",
    "codecov",
    "coverage",
    "eslint",
    "gemnasium",
    "istanbul",
    "mocha",
    "karma"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "esdoc": "^1.0.4",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "istanbul": "^0.4.5",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sauce-launcher": "^1.2.0",
    "karma-webpack": "^2.0.13",
    "mocha": "^5.0.2",
    "phantomjs-prebuilt": "^2.1.16",
    "rimraf": "^2.6.2",
    "sinon": "^5.0.3",
    "webpack": "^4.1.0"
  }
}
